<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="CLI: node" type="Application" factoryName="Application">
    <option name="MAIN_CLASS_NAME" value="org.drasyl.cli.Cli" />
    <module name="drasyl-cli" />
    <option name="PROGRAM_PARAMETERS" value="node --verbose debug" />
    <option name="WORKING_DIRECTORY" value="$PROJECT_DIR$" />
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="org.drasyl.cli.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component>
