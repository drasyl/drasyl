<component name="ProjectRunConfigurationManager">
    <configuration default="false" name="Unit Tests" type="JUnit" factoryName="JUnit" repeat_count="10">
        <useClassPathOnly/>
        <option name="PACKAGE_NAME" value=""/>
        <option name="MAIN_CLASS_NAME" value=""/>
        <option name="METHOD_NAME" value=""/>
        <option name="TEST_OBJECT" value="pattern"/>
        <option name="VM_PARAMETERS" value="-ea -XX:+ShowCodeDetailsInExceptionMessages -Dio.netty.leakDetectionLevel=PARANOID"/>
        <option name="PARAMETERS" value=""/>
        <option name="WORKING_DIRECTORY" value=""/>
        <option name="TEST_SEARCH_SCOPE">
            <value defaultName="wholeProject"/>
        </option>
        <patterns>
            <pattern testClass="^.*Test$"/>
        </patterns>
        <method v="2">
            <option name="Make" enabled="true"/>
        </method>
    </configuration>
</component>
